<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    
	<!-- When pressed, use this -->
    <item android:state_focused="false"
    	  android:state_pressed="true" 
    	  android:drawable="@drawable/activity_tab_normal_pushed"/>
    <!-- When selected, use this -->
    <item android:drawable="@drawable/activity_tab_selected"
          android:state_selected="true" />
    <!-- When not selected, use this-->
    <item android:drawable="@drawable/activity_tab_normal" />
</selector>
